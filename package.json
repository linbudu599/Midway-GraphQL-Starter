    "@midwayjs/luckyeye": "^1.0.0",
    "@midwayjs/mock": "^2.3.0",
    "@prisma/cli": "^2.15.0",
    "@prisma/client": "^2.15.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.10",
    "@types/koa-compress": "^4.0.1",
    "@types/koa-helmet": "^6.0.1",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-ratelimit": "^4.2.2",
    "@types/koa-static": "^4.0.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "14",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^9.1.0",
    "inquirer": "^7.3.3",
    "jest": "^26.4.0",
    "mwts": "^1.0.5",
    "nodemon": "^2.0.7",
    "pm2": "^4.5.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=my-midway-project --framework=@midwayjs/web",
    "stop": "egg-scripts stop --title=my-midway-project",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "dev": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts",
    "test": "midway-bin test",
    "cov": "midway-bin cov",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "check": "luckyeye",
    "voyager": "serve -s voyager/index.html",
    "docs:gen": "rimraf -rf ./documentation && graphdoc --force",
    "docs:serve": "serve -s ./documentation",
    "docs": "npm run docs:gen && npm run docs:serve",
    "prisma:generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "prisma:push": "prisma db push --preview-feature --schema ./src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate --preview-feature --schema ./src/prisma/schema.prisma"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-luckyeye": {
    "packages": [
      "midway_v2"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "linbudu599",
  "license": "MIT",
  "graphdoc": {
    "endpoint": "http://localhost:7001/graphql",
    "output": "./documentation"
  }
}
