{
  "name": "my-midway-project",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "egg-scripts start --daemon --title=my-midway-project --framework=@midwayjs/web",
    "stop": "egg-scripts stop --title=my-midway-project",
    "start_build": "yarn build && cross-env NODE_ENV=development midway-bin dev",
    "dev": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts",
    "test": "midway-bin test",
    "cov": "midway-bin cov",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "yarn cov",
    "build": "midway-bin build -c",
    "check": "luckyeye",
    "voyager": "serve -s voyager/index.html -p 8888",
    "docs:gen": "rimraf -rf ./documentation && graphdoc --force",
    "docs:serve": "serve -s ./documentation -s 7777",
    "docs": "yarn docs:gen && yarn docs:serve",
    "prisma": "yarn prisma:generate && yarn prisma:push",
    "prisma:generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema ./src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate --preview-feature --schema ./src/prisma/schema.prisma",
    "commit": "git add -A && git-cz",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn build && yarn test"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@midwayjs/core": "^2.10.18",
    "@midwayjs/decorator": "^2.10.18",
    "@midwayjs/koa": "^2.10.18",
    "@midwayjs/orm": "^1.3.1",
    "@midwayjs/web": "^2.10.19",
    "apollo-server-koa": "^2.25.0",
    "apollo-server-plugin-base": "^0.13.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "dataloader": "^2.0.0",
    "dotenv": "^10.0.0",
    "egg": "^2.29.4",
    "egg-scripts": "^2.13.0",
    "graphql": "^15.5.0",
    "graphql-query-complexity": "^0.8.1",
    "graphql-tools": "^7.0.5",
    "graphql-voyager": "^1.0.0-rc.31",
    "helmet": "^4.6.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "koa-compose": "^4.1.0",
    "koa-compress": "^5.0.1",
    "koa-helmet": "^6.1.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa-ratelimit": "^5.0.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "sqlite3": "^5.0.2",
    "type-graphql": "^1.1.1",
    "type-graphql-dataloader": "^0.3.7",
    "typeorm": "^0.2.32",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-resolvers": "1.19.2",
    "@midwayjs/cli": "^1.2.68",
    "@midwayjs/egg-ts-helper": "^1.2.0",
    "@midwayjs/luckyeye": "^1.0.2",
    "@midwayjs/mock": "^2.10.18",
    "@prisma/client": "^2.23.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/koa-compress": "^4.0.1",
    "@types/koa-helmet": "^6.0.2",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-ratelimit": "^4.2.2",
    "@types/koa-static": "^4.0.1",
    "@types/lodash": "^4.14.170",
    "@types/node": "15",
    "apollo-server-core": "^2.25.0",
    "chalk": "^4.1.1",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "fs-extra": "^10.0.0",
    "husky": "^6.0.0",
    "inquirer": "^8.1.0",
    "jest": "^27.0.1",
    "mwts": "^1.2.2",
    "nodemon": "^2.0.7",
    "pm2": "^4.5.6",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "prisma": "^2.23.0",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "ts-jest": "^27.0.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-luckyeye": {
    "packages": [
      "midway_v2"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "linbudu599",
  "license": "MIT",
  "graphdoc": {
    "endpoint": "http://localhost:7001/graphql",
    "output": "./documentation"
  }
}
